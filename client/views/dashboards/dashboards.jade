h1#dashboardTitle Dashboard
.row
  .col-xs-8.col-xs-offset-2
    form(name='profileForm', novalidate)
      table.table-striped.table-hover
        thead
          tr
            th Decision
            th Date
            th Results
          tr(ng-repeat='decision in decisions' ng-click='show(decision)')
            td {{decision.title}}
            td {{decision.date | date:'yyy-MM-dd'}}
            td {{decision.result}}
